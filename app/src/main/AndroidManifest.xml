<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xuange.phonesafe"
    android:revisionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.SettingActivity" />
        <activity android:name=".Activity.MobileSafeActivity" />
        <activity android:name=".Activity.MobileSafeSetup1Activity" />
        <activity android:name=".Activity.MobileSafeSetup2Activity" />
        <activity android:name=".Activity.MobileSafeSetup3Activity" />
        <activity android:name=".Activity.MobileSafeSetup4Activity" />
        <activity android:name=".Activity.ContactListActivity"></activity>
    </application>

</manifest>